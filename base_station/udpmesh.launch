<?xml version="1.0" ?>
<launch>
  <include file="$(find udp_mesh)/launch/rudp_mesh_node.launch">
    <!-- Wireless -->
    <!-- <arg name="mesh_interface" value="wlo1" /> -->
    <!-- Wired -->
    <arg name="mesh_interface" value="enp60s0" />
    <arg name="mesh_hostname" value="Base" />
    <arg name="topic_list" value="[1|std_msgs/Bool|estop_cmd,
                                   1|std_msgs/Bool|radio_reset_cmd,
                                   1|sensor_msgs/Joy|joy_base,
                                   2|geometry_msgs/TransformStamped|origin_from_base,
                                   2|marble_multi_agent/AgentMsg|ma_data,
                                   3|marble_multi_agent/DMReqArray|dm_request,
                                   4|marble_multi_agent/DMRespArray|dm_response]" />
    <arg name="host_list" value="[]" />

  </include>
 </launch>
